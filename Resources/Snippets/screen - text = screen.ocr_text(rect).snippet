local group, name

group = '图色'
name = 'text = screen.ocr_text(rect)'

return {
	name = string.format('%s - %s', group, name),
	arguments = {
		{type = '@rect@'},
	},
	generator = function(rect)
		return string.format('text = screen.ocr_text(%d, %d, %d, %d)', rect[1], rect[2], rect[3], rect[3])
	end,
}