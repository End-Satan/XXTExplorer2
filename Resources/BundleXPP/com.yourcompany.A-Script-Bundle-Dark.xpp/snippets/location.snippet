if not xui.get then
	xui.get = function (defaults, key)
		local t = plist.read('/private/var/mobile/Media/1ferver/uicfg/'..defaults..'.plist') or {}
		return t[key]
	end
end

return {
	name = '位置',
	arguments = {
		{
			type = 'loc',
			subtitle = '选择一个位置',
			default = (function()
				return xui.get('com.yourcompany.A-Script-Bundle', 'locationpicker')
			end)(),
		},
	},
	generator = function(loc)
		return loc
	end,
}

