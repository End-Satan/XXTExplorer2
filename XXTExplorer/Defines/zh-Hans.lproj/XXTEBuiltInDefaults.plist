			<key>enabled</key>
			<true/>
			<key>key</key>
			<string>no_idle</string>
			<key>default</key>
			<false/>
			<key>title</key>
			<string>失眠模式</string>
			<key>description</key>
			<string>防止设备进入睡眠模式 (掉线).</string>
			<key>options</key>
			<array>
				<string>关闭</string>
				<string>开启</string>
			</array>
		</dict>
		<dict>
			<key>enabled</key>
			<true/>
			<key>key</key>
			<string>script_on_daemon</string>
			<key>default</key>
			<false/>
			<key>title</key>
			<string>守护模式</string>
			<key>description</key>
			<string>如果守护进程崩溃, 尝试重启最后一次运行的脚本.</string>
			<key>options</key>
			<array>
				<string>关闭</string>
				<string>开启</string>
			</array>
		</dict>
		<dict>
			<key>enabled</key>
			<true/>
			<key>key</key>
			<string>script_end_hint</string>
			<key>default</key>
			<true/>
			<key>title</key>
			<string>脚本停止提示</string>
			<key>description</key>
			<string>显示 &quot;脚本已停止&quot; 浮动提示信息.</string>
			<key>options</key>
			<array>
				<string>关闭</string>
				<string>开启</string>
			</array>
		</dict>
		<dict>
			<key>enabled</key>
			<true/>
			<key>key</key>
			<string>no_need_pushid_alert</string>
			<key>default</key>
			<false/>
			<key>title</key>
			<string>隐藏 &quot;连接到 iTunes...&quot; 弹出框</string>
			<key>description</key>
			<string>禁止 &quot;连接到 iTunes 以使用推送通知&quot; 弹出框显示.</string>
			<key>options</key>
			<array>
				<string>关闭</string>
				<string>开启</string>
			</array>
		</dict>
		<dict>
			<key>enabled</key>
			<true/>
			<key>key</key>
			<string>no_nosim_statusbar</string>
			<key>default</key>
			<false/>
			<key>title</key>
			<string>隐藏状态栏 &quot;无 SIM&quot; 标签</string>
			<key>description</key>
			<string>禁止状态栏 &quot;无 SIM&quot; 文字标签显示.</string>
			<key>options</key>
			<array>
				<string>关闭</string>
				<string>开启</string>
			</array>
		</dict>
		<dict>
			<key>enabled</key>
			<true/>
			<key>key</key>
			<string>use_classic_control_alert</string>
			<key>default</key>
			<false/>
			<key>title</key>
			<string>使用经典提示框</string>
			<key>description</key>
			<string>在守护进程中使用经典提示框, 而不是动画提示框.</string>
			<key>options</key>
			<array>
				<string>关闭</string>
				<string>开启</string>
			</array>
		</dict>
	</array>
</dict>
</plist>
