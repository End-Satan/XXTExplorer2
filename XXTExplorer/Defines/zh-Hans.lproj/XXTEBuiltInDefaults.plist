			<string>选择启动的脚本</string>
			<key>description</key>
			<string>自动选中在文件管理器中启动过的脚本.</string>
			<key>options</key>
			<array>
				<string>关闭</string>
				<string>开启</string>
			</array>
		</dict>
		<dict>
			<key>enabled</key>
			<string>YES</string>
			<key>key</key>
			<string>XXTExplorerPasteboardDetectOnActive</string>
			<key>default</key>
			<integer>0</integer>
			<key>title</key>
			<string>唤醒时检查剪贴板可用内容</string>
			<key>description</key>
			<string>当应用程序启动或从后台唤醒时, 检查剪贴板中的授权码或可供下载的 URL.</string>
			<key>options</key>
			<array>
				<string>不检查</string>
				<string>仅检查可供下载的 URL</string>
				<string>仅检查授权码</string>
				<string>全部检查</string>
			</array>
		</dict>
	</array>
	<key>REMOTE_USER_DEFAULTS</key>
	<array>
		<dict>
			<key>enabled</key>
			<true/>
			<key>key</key>
			<string>no_nosim_alert</string>
			<key>default</key>
			<false/>
			<key>title</key>
			<string>隐藏 &quot;无 SIM&quot; 弹出框</string>
			<key>description</key>
			<string>禁止 &quot;无 SIM&quot; 弹出框显示.</string>
			<key>options</key>
			<array>
				<string>关闭</string>
				<string>开启</string>
			</array>
		</dict>
		<dict>
			<key>enabled</key>
			<true/>
			<key>key</key>
			<string>no_low_power_alert</string>
			<key>default</key>
			<false/>
			<key>title</key>
			<string>隐藏 &quot;低电量&quot; 弹出框</string>
